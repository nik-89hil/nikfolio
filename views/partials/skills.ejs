<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <link rel="stylesheet" href="/static/css/skill.css">
  
      <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
      <script type="text/javascript">
        google.charts.load("current", {packages:["corechart"]});
        google.charts.setOnLoadCallback(drawChart);
        function drawChart() {
          var data = google.visualization.arrayToDataTable([
            ['Task', 'Hours per Day'],
            ['HTML5 & CSS3',     10],
            ['React.js',      2],
            ['Javascript',  3],
            ['MongoDB', 3],
            ['Node.js & Express.js',    7]
          ]);
  
          var options = {
            title: 'Good at (%)',
            is3D: true,
          };
  
          var chart = new google.visualization.PieChart(document.getElementById('piechart_3d'));
          chart.draw(data, options);
        }
      </script>
    
    
</head>
<body>
    <div  id="skillstag"></div>
    <div class="skillContainer">
      <h1>Skills <i class="fa-brands fa-pagelines" style="color: #080808;"></i> </h1>
      <br><br>
      <div class="skilllist">
        <div id="html">HTML5 <i class="fa-brands fa-html5"></i></div>
        <div>CSS3 <i class="fa-brands fa-css3-alt"></i></div>
        <div id="bootstrap">BootStrap <i class="fa-brands fa-bootstrap"></i></div>
        <div id="javascript">JavaScript <i class="fa-brands fa-square-js"></i></div>
        <div id="rboot">React BootStrap <i class="fa-brands fa-react"></i> <i class="fa-solid fa-plus"></i> <i class="fa-brands fa-bootstrap"></i></div>
        <div id="node">Node.js <i class="fa-brands fa-node"></i></div>
        <div id="reac">React.js <i class="fa-brands fa-react"></i></div>
        <div>Express.js <i class="fa-solid fa-server"></i></div>
        <div id="mongo">MongoDB <i class="fa-brands fa-envira fa-rotate-90"></i></div>
      </div>


      <div id="piechart_3d"></div>

    </div>


</body>
</html>
